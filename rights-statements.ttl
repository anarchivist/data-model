@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix edm: <http://www.europeana.eu/schemas/edm/> .
@prefix odrl: <http://www.w3c.org/community/odrl/two/vocab/2.1/> .
@prefix premiscopy: <http://id.loc.gov/vocabulary/preservation/copyrightStatus/> .
@prefix rsdotorg: <http://rightsstatements.org/vocab/path/tba/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

rsdotorg:ic a dcterms:RightsStatement ;
     dc:title "In Copyright"@en ;
     skos:prefLabel "In Copyright (IC)"@en ;
     skos:closeMatch <http://www.europeana.eu/rights/rr-f/> ;
     skos:definition "This digital object is protected by copyright and/or related rights. Use of this digital object is subject to the licensing conditions set by the rightsholder and/or organization giving access to it."@en ;
     # how to model "more information"?
     # keep / rewrite scopeNote?
     skos:scopeNote "This rights statement is applicable when users have free (as in gratis), direct and full access to the digitised object. Access is granted subject to the licensing conditions described the on the data provider's website."@en ;
     skos:relatedMatch premiscopy:cpr ;
     dcterms:hasVersion "1.0" ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dc:identifier "ic" .

rsdotorg:ic-ow-eu a dcterms:RightsStatement ;
     dc:title "In Copyright - Orphan Works - EU"@en ;
     skos:prefLabel "In Copyright - Orphan Works - EU (IC-OW-EU)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:closeMatch <http://www.europeana.eu/rights/orphan-work-eu/> ;
     skos:definition """This item has been identified by an eligible EU organisation as an orphan work in the country of first publication and consistent with Directive 2012/28/EU of the European Parliament and of the Council of 25 October 2012 on certain permitted uses of orphan works.

For this item no rightholder(s) have been identified or, if one or more of them have been identified, none has been located despite a diligent search for the rightholders having been carried out.

If you have information about the identity or the location of the rightholder(s), please contact the Data Provider."""@en ;
     # how to model "more information"?
     # keep / rewrite scopeNote?
     skos:scopeNote "The EU Orphan works statement can be applied by eligible EU organisations to objects that have been identified as orphan works in the country of first publication and in line with the requirements of the national law implementing Directive 2012/28/EU of the European Parliament and of the Council of 25 October 2012 on certain permitted uses of orphan works."@en ;
     skos:relatedMatch premiscopy:cpr ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "ic-ow-eu" .

rsdotorg:ic-ow a dcterms:RightsStatement ;
     dc:title "In Copyright - Orphan Works"@en ;
     skos:prefLabel "In Copyright - Orphan Works (IC-OW)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition """This item has been identified by as an orphan work, which means that either no rightsholder(s) have been identified, or, if one or more of them have been identified, none has been located despite a diligent search for the rightholders having been carried out.

If you have information about the identity or the location of the rightholder(s), please contact the Data Provider."""@en ;
     # how to model "more information"?
     # scopeNote should state that this is eligible to be used in US
     skos:relatedMatch premiscopy:cpr ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "ic-ow" .

rsdotorg:ic-edu a dcterms:RightsStatement ;
     dc:title "In Copyright - Educational Use Only"@en ;
     skos:prefLabel "Copyright - Educational Use Only (IC-EDU)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition "This digital object is protected by copyright and/or related rights. For educational uses, no additional copyright permission is required from the copyright holder. Please refer to the Data Provider for additional information."@en ;
     # how to model "more information"?
     odrl:permission [
	odrl:action odrl:use ;
	odrl:constraint [
	   odrl:operator odrl:eq ;
           odrl:purpose <http://rightsstatements.org/purpose/education>
	   ]
     ] ;
     skos:relatedMatch premiscopy:cpr ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "ic-edu" .

rsdotorg:ic-nc a dcterms:RightsStatement ;
     dc:title "In Copyright - Non Commercial Use Only"@en ;
     skos:prefLabel "In Copyright - Non Commercial Use Only (IC-NC)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition "This digital object is protected by copyright and/or related rights. For noncommercial uses, no additional copyright permission is required from the copyright holder. Please refer to the Data Provider for additional information."@en ;
     # how to model "more information"?
     cc:prohibits cc:CommercialUse ;
     skos:closeMatch <http://creativecommons.org/licenses/by-nc/4.0/> ;
     skos:relatedMatch premiscopy:cpr ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "ic-nc" .

rsdotorg:ic-permission a dcterms:RightsStatement ;
     dc:title "In Copyright - Permissive Uses Available"@en ;
     skos:prefLabel "In Copyright - Permissive Uses Available (IC-Permission)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition "This digital object is protected by copyright and/or related rights. The copyright holder has made this object available subject to a license or waiver of rights that permits users to make certain uses without securing additional copyright clearance from the rightsholder (e.g., Creative Commons licenses). For more information about the permissive terms this object is subject to, please refer to Data Provider."@en ;
     # how to model "more information"?
     skos:relatedMatch premiscopy:cpr ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "ic-permission" .

rsdotorg:pd-nc a dcterms:RightsStatement ;
     dc:title "Public Domain - Non Commercial Use Only"@en ;
     skos:prefLabel "Public Domain - Non Commercial Use Only (PD-NC)"@en ;
     dcterms:modified "2014-12-18";
     skos:definition """This object is in the public domain. As part of the acquisition or digitization of this object, the contributing institution is contractually obligated to limit commercial uses.

You can, without permission, copy, modify, distribute, display, or perform the digital object, for non-commercial uses. For any other permissible uses, please review the terms and conditions of the Data Provider."""@en ;
     # how to model "more information"?
     cc:prohibits cc:CommercialUse ;
     skos:relatedMatch premiscopy:pub ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dcterms:hasVersion "1.0" ;
     dc:identifier "pd-nc" .

rsdotorg:pd-contractual-restriction a dcterms:RightsStatement ;
     dc:title "Public Domain - Other Contractual Restrictions Present"@en ;
     skos:prefLabel "Public Domain - Other Contractual Restrictions Present (PD-Contractual Restriction)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition "This object is in the public domain. As part of the acquisition or digitisation of this object, the contributing institution is contractually obligated to limit the use of this object. Limitations may be due to privacy issues, cultural protections, or donor agreements. Please refer to the Data Provider for additional information."@en ;
     # how to model "more information"?
     skos:relatedMatch premiscopy:pub ;
     dcterms:hasVersion "1.0" ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dc:identifier "pd-contractual-restriction" .

rsdotorg:pd-other a dcterms:RightsStatement ;
     dc:title "Public Domain - Other Legal Restrictions"@en ;
     skos:prefLabel "Public Domain - Other Legal Restrictions (PD-Other Legal Restrictions)"@en ;
     dcterms:modified "2014-12-18" ;
     skos:definition "This object is in the public domain, but in one or more jurisdictions, laws impose restrictions on the use of this object. Please refer to the Data Provider for additional information."@en ;
     # how to model "more information"?
     skos:relatedMatch premiscopy:pub ;
     dcterms:hasVersion "1.0" ;
     dc:creator "Digital Public Library of America and Europeana Rights Working Group" ;
     dc:identifier "pd-other" .

